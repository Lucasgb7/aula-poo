<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="lucas.aula.br.calculodearea">

    <application
        android:allowBackup="true"
        android:icon="@drawable/square"
        android:label="@string/app_name"
        android:roundIcon="@drawable/square"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".circuloPasso1" />
        <activity android:name=".retanguloPasso1" />
        <activity android:name=".trianguloPasso1" />
        <activity android:name=".circuloResultado" />
        <activity android:name=".retanguloResultado" />
        <activity android:name=".trianguloResultado"></activity>
    </application>

</manifest>